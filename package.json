{
    "name": "@z3phyro/tytra-core",
    "version": "0.1.20",
    "description": "Core project of CLI inspired by prisma, to create and mantain statically typed translations on any typescript project.",
    "files": [
        "lib"
    ],
    "main": "./lib/index.js",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "scripts": {
        "prepublish": "npm run build",
        "build": "rm -rf lib && npx tsc -p tsconfig.json",
        "postbuild": "echo '#!/usr/bin/env node' | cat - lib/index.js > temp && mv temp lib/index.js",
        "test": "jest"
    },
    "keywords": [
        "translation",
        "typed",
        "i18n",
        "typescript",
        "javascript",
        "library",
        "react",
        "svelte",
        "vue",
        "solidjs"
    ],
    "author": "@z3phyro Maykel LÃ³pez Meneses",
    "license": "ISC",
    "devDependencies": {
        "@jest/globals": "^29.5.0",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.3",
        "jest": "^29.5.0",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    }
}
